{ 
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "removeComments": false,
      "noImplicitAny": false,
      "rootDir": "./src/app",
      "esModuleInterop": true,
      "suppressImplicitAnyIndexErrors": true,
      "outDir": "./compiled"
    },
    "exclude": [ "node_modules" ] 
}